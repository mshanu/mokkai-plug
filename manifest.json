{
  "name": "mokkai-plug",
  "version": "1.0",
  "description": "This extension will give you suggestions based on your history; performs work on chrome tab events",
  "permissions": [
    "history",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus"    
  ],
  "browser_action": {
    "default_popup": "./src/main/html/main.html",
    "default_icon": "./src/main/main.png"
  },
  "manifest_version": 2
}
